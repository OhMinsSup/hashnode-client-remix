    "@tanstack/react-query": "^4.24.6",
    "@tanstack/react-query-devtools": "^4.24.6",
    "classnames": "^2.3.2",
    "cookie": "^0.5.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "isbot": "^3.6.5",
    "ky": "^0.33.2",
    "ky-universal": "^0.11.0",
    "lazysizes": "^5.3.2",
    "lodash-es": "^4.17.21",
    "rc-drawer": "^6.1.2",
    "react": "^18.2.0",
    "react-aria": "^3.22.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.1",
